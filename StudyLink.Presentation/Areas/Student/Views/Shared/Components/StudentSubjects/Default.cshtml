@model IEnumerable<StudyLink.Domain.Entities.Subject>


@if (Model.Any())
{
    <ul>
        <li><a asp-area="Student" asp-controller="Home" asp-action="Index">Home</a></li>
        @foreach (var subject in Model)
        {
            <li>
                <a asp-area="Student" asp-controller="Answer" asp-action="ListQuestionTypesWithResult" asp-route-id="@subject.SubjectId">
                    @subject.SubjectName (@subject.SubjectCode)
                </a>
            </li>
        }
    </ul>
}
else
{
    <p>No subjects assigned.</p>
}
